{
  "name": "azure-data-cleaner",
  "version": "1.0.0",
  "description": "A backend service that cleans and normalizes data using Azure OpenAI and Prisma with MySQL.",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate --schema=prisma/dms_prod.prisma && prisma generate --schema=prisma/entities.prod.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema=prisma/dms_prod.prisma && prisma migrate dev --name init --schema=prisma/entities.prod.prisma",
    "prisma:deploy": "npm run prisma:generate && npm run prisma:migrate",
    "prisma:pull": "prisma db pull --schema=prisma/dms_prod.prisma && prisma db pull --schema=prisma/entities.prod.prisma"
  },
  "keywords": [
    "express",
    "prisma",
    "openai",
    "azure",
    "data-cleaning",
    "typescript",
    "api"
  ],
  "author": "Nesru Codex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/azure-data-cleaner.git"
  },
  "dependencies": {
    "@azure/core-auth": "^1.10.0",
    "@azure/identity": "^4.10.2",
    "@azure/openai": "^2.0.0",
    "@prisma/client": "^6.18.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "fuse.js": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "openai": "^5.11.0",
    "p-limit": "^6.2.0",
    "p-retry": "^6.2.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.4.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
